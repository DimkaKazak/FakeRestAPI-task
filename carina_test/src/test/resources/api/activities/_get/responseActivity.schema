{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "ID": 0,
            "Title": "string",
            "DueDate": "2020-07-27T21:01:16.302Z",
            "Completed": true
        }
    ],
    "required": [
        "ID",
        "Title",
        "DueDate",
        "Completed"
    ],
    "properties": {
        "ID": {
            "$id": "#/properties/ID",
            "type": "integer",
            "title": "The ID schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                0
            ]
        },
        "Title": {
            "$id": "#/properties/Title",
            "type": "string",
            "title": "The Title schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "string"
            ]
        },
        "DueDate": {
            "$id": "#/properties/DueDate",
            "type": "string",
            "title": "The DueDate schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "2020-07-27T21:01:16.302Z"
            ]
        },
        "Completed": {
            "$id": "#/properties/Completed",
            "type": "boolean",
            "title": "The Completed schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        }
    },
    "additionalProperties": true
}